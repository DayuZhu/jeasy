<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <!-- <properties resource="inti.properties" /> targetRuntime="MyBatis3" -->

    <context id="mybatis" defaultModelType="flat" targetRuntime="MyBatis3">

        <property name="autoDelimitKeywords" value="true"/>
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>


        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <!--覆盖生成XML文件-->
        <plugin type="com.mybatis.generator.junit.UnmergeableXmlMappersPlugin"/>

        <plugin
                type="xin.jeasy.mybatis.generator.plugin.TemplateFileMultiPlugin">

            <property name="projectPackage"
                      value="com.sc.act.api"/>

            <property name="targetProject" value="src/test/java"/>
            <property name="templateRoot" value="scactTemplates/"/>


            <!--templatePath-mapperSuffix-fileName-targetPackage;-->
            <property name="templateInfo"
                      value="
                      AdzController.ftl-Controller-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.svc.controller.gencode;
                      AdzRequest.ftl-DtoRequest-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.common.dto.request;
                      AdzListRequest.ftl-ListDtoRequest-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.common.dto.request;
                      AdzResponse.ftl-ContentDtoResponse-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.common.dto.response;
                      AdzResponse.ftl-DtoResponse-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.common.dto.response;
                      AdzService.ftl-Service-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.svc.service;
                      AdzServiceImpl.ftl-ServiceImpl-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.svc.service.impl;
                      AdzResponse.ftl-Response-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.api.response;
                      AdzRequest.ftl-Request-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.api.request;
                      AdzRequest.ftl-ListRequest-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.api.request;
                      AdzResponse.ftl-ContentResponse-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.api.response;
                      AdzService.ftl-ApiService-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.api.service.merchant;
                      AdzServiceImpl.ftl-ApiServiceImpl-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.api.service.merchant.impl;
                      AdzController.ftl-MisController-${tableClass.shortClassName}${mapperSuffix}.java-com.zkt.idis.api.controller.merchant;"/>

        </plugin>

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 去除自动生成的注释  -->
            <property name="suppressAllComments" value="true"/>
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>

        <!-- 生成数据库字段注释及自定义注释 【扩展插件】 -->
        <!--<commentGenerator-->
        <!--type="xin.jeasy.mybatis.generator.comment.RemarksCommentGenerator">-->
        <!--&lt;!&ndash; 是否去掉注释 &ndash;&gt;-->
        <!--<property name="suppressDate" value="true"/>-->
        <!--<property name="suppressAllComments" value="true"/>-->
        <!--</commentGenerator>-->

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/sc_act?tinyInt1isBit=false"
                        userId="root" password="123456">
            <property name="useInformationSchema" value="true"/>
        </jdbcConnection>


        <javaTypeResolver type="com.mybatis.generator.junit.MyFullyQualifiesJavaType">
            <property name="forceBigDecimals" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator
                targetPackage="com.zkt.idis.svc.model"
                targetProject="src\test\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mappings/auto"
                         targetProject="src\test\resources"/>

        <javaClientGenerator
                targetPackage="com.zkt.idis.svc.dao.db.auto"
                targetProject="src\test\java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="idis_pre_stored_info" domainObjectName="PreStoredInfo">
            <generatedKey column="pre_stored_info_id" sqlStatement="MySql" identity="true"/>
        </table>

        <table tableName="idis_user_coupon" domainObjectName="UserCoupon">
            <generatedKey column="idis_user_coupon_id" sqlStatement="MySql" identity="true"/>
        </table>

    </context>
</generatorConfiguration>